{
  "name": "datarecovery-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "cross-env NG_PERSISTENT_BUILD_CACHE=1 ng serve --hmr --proxy-config src/proxy.conf.json --disable-host-check ",
    "scully": "npx scully --watch serve --proxy-config src/proxy.conf.json",
    "build:prod": "cross-env NODE_ENV=production ng build --configuration production --aot --build-optimizer",
    "build:scully": "npx scully --scanRoutes",
    "source-map-explorer": "npm run build:prod && source-map-explorer dist/datarecovery-frontend/**/*.js"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular/animations": "~13.2.2",
    "@angular/cdk": "~13.2.2",
    "@angular/common": "~13.2.2",
    "@angular/compiler": "~13.2.2",
    "@angular/core": "~13.2.2",
    "@angular/forms": "~13.2.2",
    "@angular/platform-browser": "~13.2.2",
    "@angular/platform-browser-dynamic": "~13.2.2",
    "@angular/platform-server": "^12",
    "@angular/router": "~13.2.2",
    "browser-image-compression": "^1.0.17",
    "cookieconsent": "^3.1.1",
    "ng-click-outside": "9.0.0",
    "ngx-cookieconsent": "^2.2.3",
    "ngx-page-scroll-core": "^8.0.0",
    "rxjs": "~7.5.4",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "~13.2.3",
    "@angular/compiler-cli": "~13.2.2",
    "@tailwindcss/aspect-ratio": "latest",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/line-clamp": "latest",
    "@tailwindcss/typography": "latest",
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "postcss": "^8.4.6",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^3.0.22",
    "tailwindcss-blend-mode": "^1.0.0",
    "ts-node": "~10.5.0",
    "typescript": "4.5.5",
    "@scullyio/ng-lib": "^2.1.29",
    "@scullyio/scully": "^2.1.29",
    "@scullyio/scully-plugin-puppeteer": "^2.1.29"
  }
}
